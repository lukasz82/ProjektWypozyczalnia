@model IEnumerable<ApplicationUser>

@{
    ViewData["Title"] = "Delete";
}

@if (Model.Count() == 0)
{
    <p>Brak kont użytkowników</p>
}
else
{
    foreach (ApplicationUser users in Model)
    {
    <form asp-controller="AdminPanel" asp-action="Delete" method="post">
        <p>
            @users.UserName
            @users.Id
            <input type="text" name="id" value="@users.Id">
            <input type="submit" value="Usuń" />
        </p>
    </form>
    }
}

<h2>Delete</h2>

